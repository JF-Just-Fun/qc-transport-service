  "app": {
    "number_of_threads": 0,
    "enable_session": false,
    "session_timeout": 0,
    "session_same_site": "Null",
    "session_cookie_key": "JSESSIONID",
    "session_max_age": -1,
    "document_root": "./",
    "home_page": "index.html",
    "use_implicit_page": true,
    "implicit_page": "index.html",
    "upload_path": "uploads",
    "file_types": [
      "gif",
      "png",
      "jpg",
      "js",
      "css",
      "html",
      "ico",
      "swf",
      "xap",
      "apk",
      "cur",
      "xml",
      "webp",
      "svg"
